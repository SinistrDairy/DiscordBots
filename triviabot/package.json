    "run:scripts": "cross-env TS_NODE_TRANSPILE_ONLY=true node --loader ts-node/esm run-scripts.mjs"
  },
  "keywords": [
    "typescript",
    "sern",
    "discord.js"
  ],
  "dependencies": {
    "@sern/handler": "^4.2.4",
    "@sern/publisher": "^1.1.4",
    "all": "^0.0.0",
