{
  "name": "russell",
  "version": "1.7.21",
  "private": true,
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf ./dist && sern build",
    "start": "node .",
    "commands:publish": "npm run build && sern commands publish",
