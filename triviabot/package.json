    "dotenv": "^16.5.0",
    "g": "^2.0.1",
    "mongodb": "^6.16.0",
    "mongoose": "^8.15.0",
    "node-cron": "^4.0.7"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^10.0.1",
    "@sern/cli": "^1.4.0",
    "@types/node": "^22.15.21",
    "cross-env": "^7.0.3",
    "release-it": "^19.0.3",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8"
  },
  "type": "module"
